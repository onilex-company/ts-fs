{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "moduleResolution": "Node16",
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "downlevelIteration": true,
      "baseUrl": "./",
      "skipLibCheck": true,
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }